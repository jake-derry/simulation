<?xml version="1.0"?>
<Simulation>
    <type>Foraging</type>
    <author>
        Jonah Knapp
    </author>
    <size>
        <rows>50</rows>
        <columns>50</columns>
    </size>
    <defaultState>empty</defaultState>
    <cellDistribution>cell</cellDistribution>
    <activeCells>
        <random>
            <state>food</state> <value>10</value>
        </random>
        <random>
            <state>nest</state> <value>1</value>
        </random>
        <cell> <row>5</row> <col>5</col> <state>nest</state></cell>
        <cell> <row>40</row> <col>40</col> <state>food</state></cell>
        <cell> <row>20</row> <col>20</col> <state>food</state></cell>

    </activeCells>
    <foragingPheromones>5</foragingPheromones>
    <returningPheromones>10</returningPheromones>
    <birthProbability>30</birthProbability>

    <shape>rectangle</shape>
    <neighbor> <index>0</index> </neighbor>
    <neighbor> <index>1</index> </neighbor>
    <neighbor> <index>2</index> </neighbor>
    <neighbor> <index>3</index> </neighbor>
    <neighbor> <index>4</index> </neighbor>
    <neighbor> <index>5</index> </neighbor>
    <neighbor> <index>6</index> </neighbor>
    <neighbor> <index>7</index> </neighbor>
    <Style>styles/Style1.xml</Style>
</Simulation>